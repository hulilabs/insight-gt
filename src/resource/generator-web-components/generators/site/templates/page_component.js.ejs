    'vue',
    'web-components/<%= componentNameLowerCasePlural %>/<%= componentNameLowerCase %>_component',
    'web-components/markdown/markdown_component',
    'text!web-components/<%= componentNameLowerCasePlural %>/readme.md',
    'text!pages/<%= componentNameLowerCase %>/<%= componentNameLowerCase %>-page_template.html'
], function(
    Vue,
    <%= componentNameTitleCase %>,
    Markdown,
    <%= componentNameTitleCase %>Readme,
    Template
) {

    var <%= componentNameTitleCase %>Page = Vue.extend({
        template : Template,
        data : function() {
            return {
                state : {
                },
                markdownSource : {
                    documentation : <%= componentNameTitleCase %>Readme
                }
            };
        },
        methods : {
        },
        components : {
            'wc-<%= componentNameLowerCase %>' : <%= componentNameTitleCase %>,
            'wc-markdown' : Markdown
        }
    });

    return <%= componentNameTitleCase %>Page;
});
